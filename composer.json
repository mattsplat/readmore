{
    "name": "mattsplat/readmore",
    "description": "A Laravel Nova field to hide long text and show when clicked.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Mattsplat\\Readmore\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mattsplat\\Readmore\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
